{
  "name": "everybody777-frontend",
  "private": true,
  "version": "0.1.0",
  "repository": "https://github.com/HollysParty/everybody777-frontend.git",
  "license": "MIT",
  "maintainers": [
    "mysticPrg",
    "sunny kyung"
  ],
  "scripts": {
    "start": "lerna run --scope @service/* --parallel --stream start",
    "start:monitor": "lerna run --scope @service/monitor --stream start",
    "start:paint": "lerna run --scope @service/paint --stream start",
    "build": "lerna run --scope @service/* --parallel --stream build",
    "build:monitor": "lerna run --scope @service/monitor --stream build",
    "build:paint": "lerna run --scope @service/paint --stream build"
  },
  "workspaces": {
    "packages": [
      "packages/*",
      "services/*"
    ]
  },
  "devDependencies": {
    "lerna": "^3.22.1"
  }
}
